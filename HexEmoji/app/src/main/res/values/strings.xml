    <string name="emoji_category_description_people_emotions">
        People and different facial expressions, such as smiles, laughter, sadness, surprise,
        anger, among others.
    </string>
    <string name="emoji_category_description_nature">
        Emojis representing animals, insects, flowers, trees, and natural elements, like the sun,
        moon, and rain.
    </string>
    <string name="emoji_category_description_food">
        Emojis related to food, drinks, and treats, such as pizza, burgers, ice cream,
        coffee, among others.
    </string>
    <string name="emoji_category_description_activities">
        Emojis that represent activities, hobbies, sports, musical instruments, games, and
        other forms of entertainment.
    </string>
    <string name="emoji_category_description_places_travel">
        Travel, locations, modes of transport, and various buildings, such as airplanes, cars,
        buildings, and monuments.
    </string>
    <string name="emoji_category_description_objects">
        Emojis for household items, celebrations, and the most diverse set of objects.
    </string>
    <string name="emoji_category_description_symbols">
        Includes emojis with different shapes and colors of hearts, as well as signs, symbols, and shapes.
    </string>
    <string name="emoji_category_description_flags">
        A group of flags from various countries and locations.
    </string>
    <string name="emoji_category_description_numbers_letters">
        Emojis for the numbers 0 to 9 and the 26 letters of the Latin alphabet.
    </string>

